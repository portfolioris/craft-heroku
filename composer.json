{
  "name": "craftcms/craft",
  "description": "Craft CMS.",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "project"
  ],
  "license": "MIT",
  "homepage": "https://craftcms.com/",
  "type": "project",
  "support": {
    "email": "support@craftcms.com",
    "issues": "https://github.com/craftcms/cms/issues",
    "forum": "https://craftcms.stackexchange.com/",
    "source": "https://github.com/craftcms/cms",
    "docs": "https://craftcms.com/docs",
    "rss": "https://craftcms.com/changelog.rss"
  },
  "require": {
    "craftcms/cms": "3.0.36",
    "vlucas/phpdotenv": "^2.4.0",
    "putyourlightson/craft-blitz": "^1.4",
    "nystudio107/craft-twigprofiler": "^1.0",
    "ext-mbstring": "*",
    "ext-imagick": "*",
    "craftcms/redactor": "^2.1",
    "timkelty/craft3-cloudinary": "^1.0",
    "tomdiggle/craft-gatekeeper": "^1.0",
    "markhuot/craftql": "1.2.1",
    "craftcms/element-api": "2.5.4",
    "fruitstudios/linkit": "1.1.4",
    "jungleminds/nethooks": "1.1.2",
    "sebastianlenz/linkfield": "1.0.15"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@composer dump-autoload -o",
      "@php craft setup/welcome"
    ]
  }
}
